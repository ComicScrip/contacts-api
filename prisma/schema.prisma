generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model contacts {
  id         Int     @id @default(autoincrement())
  email      String  @unique
  first_name String? @default("")
  last_name  String? @default("")
}

model migrations {
  id     Int      @id @default(autoincrement())
  name   String
  run_on DateTime
}
